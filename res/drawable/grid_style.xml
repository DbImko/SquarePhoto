<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<gradient android:startColor="#00000000" android:endColor="#00000000"
				android:angle="270" />
			<corners android:bottomRightRadius="13dp"
				android:bottomLeftRadius="13dp" android:topLeftRadius="13dp"
				android:topRightRadius="13dp" />
			<stroke android:color="#4551FF" android:width="2px" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient android:startColor="#ffffff" android:endColor="#70FFFFFF"
				android:angle="270" />
			<corners android:bottomRightRadius="13dp"
				android:bottomLeftRadius="13dp" android:topLeftRadius="13dp"
				android:topRightRadius="13dp" />
			<stroke android:color="#000000" android:width="1px" />
		</shape>
	</item>
</selector>
